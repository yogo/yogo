<iframe id="editor_frame"></iframe>

<script>
    var baseUrl = '/projects';
    var projectId = <%= @project.id %>;
    var editor_frame = $('iframe#editor_frame');
    editor_frame.attr('src', '/model_editor.html#projects/' + projectId);
    editor_frame.load(function() {
        console.debug(this);
        this.contentWindow.ModelEditor.mainDataLoad(baseUrl, projectId);
    });
</script>