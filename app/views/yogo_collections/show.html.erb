<H3><%= @model.name.split("::")[-1].titleize %></H3>
<table>
  <thead>
    <tr>
      <th></th><th></th>
      <%- @model.properties.each do |h| %>
      <th><%= h.name.to_s.titleize %></th>
      <%- end %>
    </tr>
  </thead>
  <tbody>
  <%- @data.each do |d| %>
    <tr>
      <td><%= link_to('Edit', edit_project_yogo_collection_yogo_data_path(@project, @model.name.split("::")[-1], d.id)) %></td>
      <td><%= link_to('Delete', project_yogo_collection_yogo_data_path(@project, @model.name.split("::")[-1], d.id), :method => :delete) %></td>
      <%- @model.properties.each do |p| %>
      <td><%= d[p.name] %></td>
      <%- end %>
    </tr>
    <%- end %>
  </tbody>
</table>
<%= pagination_links(@data, params[:page], 5) %>
<%= render :partial => 'csv_form'%>